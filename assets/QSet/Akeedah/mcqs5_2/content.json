          {
            "correct": false,
            "tipsAndFeedback": {
              "tip": "",
              "chosenFeedback": "<p style=\"text-align:right\">هذه الآية تدعو إلى النظر في عاقبة المكذبين، وليس التفكر في خلق السماوات والأرض.</p>",
              "notChosenFeedback": ""

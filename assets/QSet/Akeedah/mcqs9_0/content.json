            "correct": true,
            "tipsAndFeedback": {
              "tip": "",
              "chosenFeedback": "<p style=\"text-align:right\">صحيح! هذه الآية تشير بشكل مباشر إلى الفطرة والخلق الإلهي.</p>",
              "notChosenFeedback": ""
            },
            "text": "<p style=\"text-align:right\">﴿وَنَفْسٍ وَمَا سَوَّاهَا﴾</p>"

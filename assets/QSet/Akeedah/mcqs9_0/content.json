            "tipsAndFeedback": {
              "tip": "",
              "chosenFeedback": "<p style=\"text-align:right\">هذه الآية تشير إلى الأمور التي تعود إلى الله وليس إلى الفطرة.</p>",
              "notChosenFeedback": ""
            },
            "text": "<p style=\"text-align:right\">﴿إِنَّمَا أَمْرُهُ إِلَى اللَّهِ﴾</p>"
          }
